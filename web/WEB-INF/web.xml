<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <context-param>
        <param-name>view-prefix</param-name>
        <param-value>/</param-value>
    </context-param>
    <context-param>
        <param-name>view-suffix</param-name>
        <param-value>.html</param-value>
    </context-param>
    <servlet>
        <servlet-name>addServlet</servlet-name>
        <servlet-class>com.atguigu.demo.AddServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>demo01Servlet</servlet-name>
        <servlet-class>com.atguigu.demo.Demo01Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>demo02Servlet</servlet-name>
        <servlet-class>com.atguigu.demo.Demo02Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>demo03Servlet</servlet-name>
        <servlet-class>com.atguigu.demo.Demo03Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>demo04</servlet-name>
        <servlet-class>com.atguigu.demo.Demo04</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>demo05Servlet</servlet-name>
        <servlet-class>com.atguigu.demo.Demo05Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>demo06Servlet</servlet-name>
        <servlet-class>com.atguigu.demo.Demo06Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>demo07Servlet</servlet-name>
        <servlet-class>com.atguigu.demo.Demo07Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addServlet</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>demo01Servlet</servlet-name>
        <url-pattern>/demo01</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>demo02Servlet</servlet-name>
        <url-pattern>/demo02</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>demo03Servlet</servlet-name>
        <url-pattern>/demo03</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>demo04</servlet-name>
        <url-pattern>/demo04</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>demo05Servlet</servlet-name>
        <url-pattern>/demo05</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>demo06Servlet</servlet-name>
        <url-pattern>/demo06</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>demo07Servlet</servlet-name>
        <url-pattern>/demo07</url-pattern>
    </servlet-mapping>
</web-app>